# Default variables
@VARIABLES@
# TOP source directory.
topdir = @topdir@
top_builddir = $(topdir)

subdirs = @USB_TYPE@

# Include the default make targets: to be put before the all-targets: rule.
@TARGETS@

apclib = libusb.a

apclibobjs = usb.o

all-targets: $(apclib)

$(apclib): $(apclibobjs) @USB_TYPE@/libpusb.a
	rm -f $(apclib)
	ar rc $(apclib) $(apclibobjs)
	@RANLIB@ $(apclib)

clean: targetclean

distclean: targetdistclean
	$(RMF) Makefile

install:

uninstall:

